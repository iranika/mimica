export class TemplateStore{
    private static _instance: TemplateStore;
    
    // eslint-disable-next-line @typescript-eslint/no-empty-function
    private constructor(){};

    public static get getInstance(): TemplateStore{
        if (!this._instance){
            console.log("create instance of DrawerStore")
            this._instance = new TemplateStore();
        }
        return this._instance;
    }
}